{
  "name": "irisclairedigitalix",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "increment": "node ./IncrementVersion.js",
    "androidAab": "npm run increment && eas build --profile production --platform android",
    "androidDev": "npm run increment && eas build --profile development --platform android"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "hooks": {
    "pre-commit": "npm test"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "verbose": true,
    "resetMocks": true,
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native(-.*)?|@react-native(-community)?|@react-navigation|@rneui?|@react-native-vector-icons|@firebase?|firebase?|expo-constants|expo-modules-core|@sentry/.*|expo-linear-gradient)/)"
    ],
    "transform": {
      "^.+\\.(js|ts|tsx)$": "babel-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/android/",
      "/ios/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@expo-google-fonts/mulish": "^0.2.3",
    "@expo-google-fonts/oswald": "^0.2.3",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/stack": "^6.3.16",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.7",
    "axios": "^1.4.0",
    "dotenv": "^16.0.3",
    "expo": "^49.0.6",
    "expo-constants": "~14.4.2",
    "expo-dev-client": "~2.4.11",
    "expo-device": "~5.4.0",
    "expo-font": "~11.4.0",
    "expo-linear-gradient": "~12.3.0",
    "expo-localization": "~14.3.0",
    "expo-notifications": "~0.20.1",
    "expo-secure-store": "~12.3.1",
    "expo-sharing": "~11.5.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-updates": "~0.18.14",
    "firebase": "^9.21.0",
    "lottie-react-native": "5.1.6",
    "openai": "^3.2.1",
    "protobufjs": "^7.2.4",
    "react": "18.2.0",
    "react-native": "0.72.5",
    "react-native-flip-card": "^3.5.7",
    "react-native-form-component": "^2.6.10",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-global-state-hooks": "^5.0.14",
    "react-native-purchases": "^6.6.2",
    "react-native-reanimated": "~3.3.0",
    "react-native-reanimated-carousel": "^3.3.2",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-view-shot": "3.7.0",
    "react-native-web": "~0.19.6",
    "uuid": "^9.0.0",
    "zodiac-signs": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-modules-commonjs": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@expo/metro-config": "^0.10.7",
    "@firebase/rules-unit-testing": "^2.0.7",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/react-native": "^12.1.2",
    "@types/jest": "^29.5.2",
    "@types/react": "~18.2.14",
    "@types/react-native-flip-card": "^3.5.2",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.5.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-expo": "^49.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "private": true
}
